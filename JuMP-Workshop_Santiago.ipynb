    "        push!(surface_clusterblocks[c],i)\n",
    "      end\n",
    "    end\n",
    "  end\n",
    "  return benchphase,ùùπ,clusterblocks,blockcluster,edges,x_C,surface_clusterblocks\n",
    "end\n"
