    "  if freshstart\n",
    "    @variable(m, x[j=1:length( ñ),t=1:(T-œÑ+1)] ,Bin);\n",
    "  else\n",
    "    @variable(m, x[j=1:length( ñ),t=1:(T-œÑ+1)] ,Bin, start=Int(round( min(xdict[(j,t+œÑ-1)],‚Ñì[j]*1.0) )));\n",
    "  end\n",
    "  @variable(m, lowerbound <=z<= sum(Œ©ext) * 10^5)\n",
